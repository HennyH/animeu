    "url": "https://github.com/HennyH/animeu/issues"
  },
  "homepage": "https://github.com/HennyH/animeu#readme",
  "devDependencies": {
    "@types/jquery": "^3.3.29",
    "autoprefixer": "^9.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
