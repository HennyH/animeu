{
  "name": "animeu",
  "version": "0.0.1",
  "description": "Anime character ranking website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p",
    "wds": "webpack-dev-server",
    "flask": "cross-env FLASK_APP=animeu.app flask run",
    "dev": "cross-env FLASK_ENV=development run-p wds flask",
    "lint:ts": "tslint --format stylish animeu/**/*.ts",
    "lint:py:pylint": "pylint animeu",
    "lint:py:pydocstyle": "pydocstyle animeu",
    "lint:py": "run-s -c lint:py:*",
    "lint": "run-s -c lint:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HennyH/animeu.git"
  },
